    "react": "16.11.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.11.0",
    "react-native": "0.62.0",
    "react-native-elements": "^1.2.7",
    "react-native-vector-icons": "^6.6.0",
    "react-router-native": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.5",
