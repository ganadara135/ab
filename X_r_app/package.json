      "**/apollo-link-http",
      "**/apollo-link-http/**",
      "**/apollo-client",
      "**/apollo-client/**",
      "**/formik",
      "**/formik/**",
      "**/graphql",
      "**/graphql/**",
      "**/graphql-tag",
      "**/graphql-tag/**",
      "**/@abb/common",
      "**/@abb/common/**",
      "**/@abb/controller",
      "**/@abb/controller/**",
      "**/react-router-native",
      "**/react-router-native/**",
      "**/react-router",
      "**/react-router/**",
      "**/react-native",
      "**/react-native/**",
      "**/react-native-elements",
      "**/react-native-elements/**",
      "**/react-native-unimodules",
      "**/react-native-unimodules/**",
      "**/react-native-community",
      "**/react-native-community/**",
      "**/react-native*",
      "**/react-native*/**",
      "**/expo",
      "**/expo/**",
      "**/react-native-typescript-transformer",
      "**/react-native-typescript-transformer/**",
      "**/metro-bundler-config-yarn-workspaces",
      "**/metro-bundler-config-yarn-workspaces/**"
