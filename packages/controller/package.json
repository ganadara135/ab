{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "schema:download": "npx apollo client:download-schema --endpoint=http://localhost:4000 schema.json",
    "schema:downloadFromEx": "npx apollo client:download-schema --endpoint=http://49.50.163.26:4000 schema.graphql",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=./schema.json --target=typescript --includes=src/**/*.tsx --addTypename --outputFlat=./src/schemaTypes.ts",
    "gen:types": "yarn schema:download && yarn codegen:generate",
    "refresh:types": "yarn gen:types && yarn build",
    "cleanController": "rm -rf node_modules ../../node_modules ../../yarn.lock"
  },
  "dependencies": {
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "16.9.0",
    "react-apollo": "3.1.1",
    "react-dom": "16.9.0",
    
    "@apollo/react-hooks":"~3.1.4"
  },
  "devDependencies": {
    "@babel/core": "~7.6.0",
    "@babel/runtime": "^7.9.2",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.0",
    "apollo": "2.26.0",
    "apollo-codegen": "^0.20.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.6.3"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.0"
  },
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/rimraf",
      "**/rimraf/**",
      "**/typescript",
      "**/typescript/**",
      "**/tslint",
      "**/@babel/runtime",
      "**/@babel/runtime/**"
    ]
  }
}
