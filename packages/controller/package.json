{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "introspect": "npx apollo client:download-schema --endpoint=http://localhost:4000 schema.graphql",
    "generate": "npx apollo client:codegen --target typescript --localSchemaFile ./schema.graphql",
    "gen-types": "yarn introspect && yarn generate"
  },
  "dependencies": {
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "react": "16.9.0",
    "react-apollo": "3.0.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.7",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "apollo-codegen": "^0.20.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "private": true,
  "workspaces": {
    "nohoist": [
      "**/rimraf",
      "**/rimraf/**"
    ]
  }
}
